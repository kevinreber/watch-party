{
  "name": "watch-party",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel dev:convex dev:app",
    "dev:app": "node --import tsx ./server.ts",
    "dev:convex": "convex dev",
    "build": "convex deploy --cmd 'npx convex codegen && react-router build'",
    "start": "NODE_ENV=production node --import tsx ./server.ts",
    "typecheck": "react-router typegen && tsc",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:seed": "npx convex run testing:seedTestUsers && npx convex run testing:seedTestRooms && npx convex run testing:seedTestFriendData",
    "test:clear": "npx convex run testing:clearTestData"
  },
  "dependencies": {
    "@clerk/clerk-react": "^5.0.0",
    "@emotion/react": "^11.14.0",
    "@emotion/styled": "^11.14.0",
    "@mui/icons-material": "^5.16.0",
    "@mui/material": "^5.16.0",
    "@react-router/express": "^7.1.1",
    "@react-router/node": "^7.1.1",
    "ably": "^2.17.0",
    "convex": "^1.17.0",
    "express": "^4.21.0",
    "get-youtube-id": "^1.0.1",
    "isbot": "^5.1.17",
    "moment": "^2.30.0",
    "notistack": "^3.0.1",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-player": "^2.16.0",
    "react-router": "^7.1.1"
  },
  "devDependencies": {
    "@playwright/test": "^1.57.0",
    "@react-router/dev": "^7.1.1",
    "@types/express": "^4.17.21",
    "@types/node": "^20.0.0",
    "@types/react": "^18.3.1",
    "@types/react-dom": "^18.3.1",
    "npm-run-all": "^4.1.5",
    "tsx": "^4.7.0",
    "typescript": "^5.1.0",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^4.0.17"
  }
}
